{
  "name": "stencil-vizzle",
  "version": "0.0.2",
  "description": "Visualization web components built with Stencil.js and D3.",
  "author": "Mark Fehrenbacher <kendopunk@hotmail.com>",
  "keywords": [
    "Stencil.js",
    "D3.js",
    "web components",
    "data visualization",
    "charts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kendopunk/stencil-vizzle.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-vizzle/stencil-vizzle.js",
  "files": [
    "dist/",
    "loader/",
    "demo/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "coverage": "stencil test --spec --coverage",
    "demo": "npm run build && node_modules/.bin/http-server ./demo -a localhost -p 5150 -o",
    "e2e": "stencil test --e2e",
    "e2e:watch": "stencil test --e2e --watchAll",
    "lint": "npm run lint:stencil && npm run lint:scss",
    "lint:stencil": "tslint --project tsconfig.json",
    "lint:scss": "prettier --write \"src/**/*.scss\"",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^1.5.1",
    "@stencil/sass": "^1.0.1",
    "@types/jest": "24.0.15",
    "@types/puppeteer": "1.20.1",
    "http-server": "^0.11.1",
    "husky": "^3.0.9",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "kebab-case": "^1.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "puppeteer": "1.20.0",
    "tslint": "^5.20.0",
    "tslint-ionic-rules": "0.0.21",
    "tslint-stencil": "^1.0.1"
  },
  "license": "MIT",
  "dependencies": {
    "d3": "^7.6.1",
    "d3-array": "^2.3.1",
    "d3-axis": "^1.0.12",
    "d3-collection": "^1.0.7",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-shape": "^1.3.5",
    "d3-transition": "^3.0.1",
    "lodash": "^4.17.15",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
